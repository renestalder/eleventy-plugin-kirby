{
  "name": "eleventy-plugin-kirby",
  "version": "0.2.12",
  "description": "Eleventy (11ty) plugin to make the work with Kirby CMS easier",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build && npm test",
    "test": "ava test/index.js",
    "lint-staged": "lint-staged",
    "build": "tsc",
    "release": "standard-version"
  },
  "hooks": {
    "pre-commit": "npm run lint-staged",
    "pre-push": "npm run test"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "kirby",
    "kirbycms",
    "getkirby"
  ],
  "author": "RenÃ© Stalder <mail@renestalder.me>",
  "license": "MIT",
  "homepage": "https://gitlab.com/renestalder/eleventy-plugin-kirby",
  "repository": "gitlab:renestalder/eleventy-plugin-kirby",
  "peerDependencies": {
    "@11ty/eleventy": ">=0.11.0"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "node-fetch": "^2.6.1",
    "normalizr": "^3.6.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "ava": "^3.15.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3"
  },
  "ava": {
    "failFast": false,
    "files": [
      "./test/*.js"
    ]
  },
  "lint-staged": {
    "*.{js,css,md,json}": [
      "prettier --write"
    ]
  }
}
